<html>
    <head>
        <title><%= @title %></title>
    </head>
    <body>
        <h1><%= @title %></h1>
        <table border="1">
            <tr><th>Key</th><th>Value</th></tr>
            <% @env.each do |key, value| %>
                <tr><td><%= key %></td><td><%= value %></td></tr>
            <% end %>
        </table>
    </body>
</html>
